# Development Environment Configuration
# Override .env settings for development environment

NODE_ENV=development
PORT=3001

# Database (local development)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=travel_support_dev
DB_USER=postgres
DB_PASSWORD=your_dev_password
DB_SSL=false

# Enable debug logging in development
LOG_LEVEL=debug

# Frontend URL (local dev)
FRONTEND_URL=http://localhost:3000

# AI Features (enabled for testing)
AI_FEATURES_ENABLED=true
AI_ROUTE_OPTIMIZATION=true
AI_MAX_DAILY_COST=1

# Redis (optional - local instance)
REDIS_HOST=localhost
REDIS_PORT=6379
