# Staging Environment Configuration
# Override .env settings for staging environment

NODE_ENV=staging
PORT=3001

# Database (staging server)
DB_HOST=staging-db.example.com
DB_PORT=5432
DB_NAME=travel_support_staging
DB_USER=postgres
DB_PASSWORD=your_staging_password
DB_SSL=true

# Logging
LOG_LEVEL=info

# Frontend URL (staging)
FRONTEND_URL=https://staging.example.com

# AI Features (enabled for testing)
AI_FEATURES_ENABLED=true
AI_ROUTE_OPTIMIZATION=true
AI_MAX_DAILY_COST=5

# Redis (staging instance)
REDIS_HOST=staging-redis.example.com
REDIS_PORT=6379
