{
  "name": "@travel-support/frontend",
  "version": "2.0.0",
  "description": "Travel Support System - Frontend Application",
  "type": "module",
  "scripts": {
    "dev": "set NODE_OPTIONS=--max-old-space-size=4096 && vite",
